{
  "name": "@codesee/tracker",
  "version": "0.160.0",
  "description": "Provides an API for web scripts to tap into CodeSee event streams.",
  "type": "commonjs",
  "license": "UNLICENSED",
  "keywords": [
    "codesee"
  ],
  "main": "build/codesee.node.js",
  "browser": {
    "./build/codesee.node.js": "./build/codesee.web.js",
    "./build/codesee.node.hosted.js": "./build/codesee.web.hosted.js"
  },
  "externals": [],
  "prettier": "@codesee/prettier-config",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@codesee/batcher": "^0.160.0",
    "@codesee/fab": "^0.160.0",
    "@codesee/prettier-config": "^0.160.0",
    "@codesee/serialization": "^0.160.0",
    "@codesee/shared-constants": "^0.160.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^24.9.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.4",
    "node-fetch": "^2.6.1",
    "prettier": "2.2.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "files": [
    "build",
    "hosted",
    "jest-teardown.js"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --fix",
    "format": "prettier --write 'src/**/*.js' '__tests__/**/*.js'",
    "pre-commit": "lint-staged",
    "build:dev": "webpack --env=dev --target=web",
    "build:dev:hosted": "webpack --env=dev --target=web --hosted=true",
    "build:prod": "webpack --env=prod --target=web",
    "build:prod:hosted": "webpack --env=prod --target=web --hosted=true",
    "build:node": "webpack --env=dev --target=node",
    "build:node:hosted": "webpack --env=dev --target=node --hosted=true",
    "build:node:prod": "webpack --env=prod --target=node",
    "build:node:prod:hosted": "webpack --env=prod --target=node --hosted=true",
    "build:pack": "node scripts/pack.js",
    "test": "node_modules/.bin/jest",
    "watch": "node_modules/.bin/jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "danger:release": "npm run build:prod && npm run build:node:prod && npm publish --access public"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "__tests__/**/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "author": "CodeSee"
}
