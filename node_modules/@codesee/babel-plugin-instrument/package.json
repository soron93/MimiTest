{
  "name": "@codesee/babel-plugin-instrument",
  "version": "0.160.0",
  "description": "Instruments javascript code to report internal execution and state for use with CodeSee Visualizations.",
  "type": "commonjs",
  "main": "lib/index.js",
  "license": "UNLICENSED",
  "keywords": [
    "babel-plugin",
    "codesee"
  ],
  "prettier": "@codesee/prettier-config",
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "array.prototype.flatmap": "^1.2.3",
    "jest-util": "^27.0.2",
    "object-path": "^0.11.4",
    "pkg-up": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.0",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-spread": "7.12.1",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.12.10",
    "@codesee/prettier-config": "^0.160.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react-app": "9.1.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.5.4",
    "next": "^11.1.0",
    "prettier": "2.2.1"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --fix",
    "format": "prettier --write 'src/**/*.js' '__tests__/**/*.js'",
    "pre-commit": "lint-staged",
    "build:pack": "npm pack",
    "build:prod": "npx babel ./src --out-dir ./lib --config-file ./.babelrc.prod.js --no-comments",
    "build": "npx babel ./src --out-dir ./lib",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "watch": "node_modules/.bin/jest --watch",
    "danger:release": "npm run build:prod && npm publish --access public"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "__tests__/**/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "author": "CodeSee"
}
